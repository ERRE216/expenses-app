{
  "expo": {
    "name": "Expenses Tracker",
    "slug": "expenses-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F2F2F2"
    },
    "updates": {
